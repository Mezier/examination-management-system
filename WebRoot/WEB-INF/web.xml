<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<filter>
	<filter-name>CodeFilter</filter-name>
	<filter-class>seecen.filter.CodeFilter</filter-class>
	<init-param>
	       <param-name>code</param-name>
	       <param-value>utf-8</param-value>
	   </init-param>
	</filter>
	<filter-mapping>
	<filter-name>CodeFilter</filter-name>
	<url-pattern>/*</url-pattern>
	</filter-mapping>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RegStudentServlet</servlet-name>
    <servlet-class>seecen.learning.servlet.RegStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AuthSystemServlet</servlet-name>
    <servlet-class>seecen.system.servlet.AuthSystemServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RoleSystemServlet</servlet-name>
    <servlet-class>seecen.system.servlet.RoleSystemServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>subjectServlet</servlet-name>
    <servlet-class>seecen.exam.servlet.subjectServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>QuestionServlet</servlet-name>
    <servlet-class>seecen.exam.servlet.QuestionServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MoldSevlet</servlet-name>
    <servlet-class>seecen.exam.servlet.MoldSevlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ExamServlet</servlet-name>
    <servlet-class>seecen.exam.servlet.ExamServlet</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>RegStudentServlet</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AuthSystemServlet</servlet-name>
    <url-pattern>/system/auth</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RoleSystemServlet</servlet-name>
    <url-pattern>/system/role</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>subjectServlet</servlet-name>
    <url-pattern>/system/exam/subject</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QuestionServlet</servlet-name>
    <url-pattern>/system/exam/question</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MoldSevlet</servlet-name>
    <url-pattern>/system/exam/mold</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ExamServlet</servlet-name>
    <url-pattern>/system/exam/exam</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
